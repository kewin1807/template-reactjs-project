  "scripts": {
    "start": "cross-env REACT_APP_LOGIN=0 GENERATE_SOURCEMAP=true BUILD_PATH=www PORT=8080 craco start",
    "start:login": "cross-env GENERATE_SOURCEMAP=true BUILD_PATH=www HTTPS=true PORT=443 HOST=flysaas-gcc.sharepointguild.com craco start",
    "mock": "cross-env REACT_APP_LOGIN=0 GENERATE_SOURCEMAP=true BUILD_PATH=www PORT=9090 craco start",
    "postinstall": "node_modules/.bin/webpack  --config webpack.dll.config.js",
    "dll": "node webpack/dll/build.js",
    "build": "cross-env REACT_APP_LOGIN=0 GENERATE_SOURCEMAP=false BUILD_PATH=www craco build",
