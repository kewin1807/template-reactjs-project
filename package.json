    "lint-fix:script": "yarn lint:script --fix",
    "lint-fix:style": "yarn lint:style --fix",
    "lint:script": "eslint --max-warnings 0 src/** --ext '.js,.jsx,.ts,.tsx'",
    "lint:style": "stylelint src/**/*.scss",
    "lint-bin-fix:script": "yarn lint-bin:script --fix",
    "lint-bin:script": "eslint --max-warnings 0 bin/** --ext '.js,.jsx,.ts,.tsx'",
    "lint-webpack-fix:script": "yarn lint-webpack:script --fix",
    "lint-webpack:script": "eslint --max-warnings 0 webpack/**/*.js craco.config.js",
    "pretty-quick": "pretty-quick",
    "tsc": "tsc --noEmit",
    "i18n:common": "node webpack/i18n/is-common.js",
    "i18n:valid": "node webpack/i18n/is-use.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
